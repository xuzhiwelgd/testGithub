/*********************timer**********************/




.chk_3{ display:none;}
.chk_3 + label {
	background-color: #d6d6d6;
	width:0.90rem;
	height:0.42rem;
	border-radius:0.36rem;
	display: inline-block;
	position: relative;
	-webkit-transition: all 0s ease-in;
	transition: all 0s ease-in;
	cursor:pointer}

.chk_3 + label:after {
	content: ' ';
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	border-radius: 0.36rem;
	}

.chk_3 + label:before {
	content: ' ';
	position: absolute;
	background: white;
	top: 0.02rem;
	left: 0.02rem;
	z-index: 999999;
	width:0.36rem;
	-webkit-transition: all 0s ease-in;
	transition: all 0s ease-in;
	height:0.36rem;
	border-radius: 0.36rem;
	box-shadow: 0 2px 1px rgba(0,0,0,0.3), 0 0px 1px rgba(0,0,0,0.3);}

.chk_3:active + label:after {
	box-shadow: inset 0 0 0 20px #eee, 0 0 1px #eee;}

.chk_3:active + label:before {
	width:0.36rem;}

.chk_3:checked:active + label:before {
	width:0.36rem;
	left:0.51rem}

.chk_3  + label:active {
	}

.chk_3:checked + label:before {
	content: ' ';
	position: absolute;
	left:0.52rem;
	border-radius:0.36rem;}

.chk_3:checked + label:after {
	content: ' ';
	font-size: 1.5em;
	position: absolute;
	background: #007AFF;
	}
	
	
